{
    "name": "enhancedreality/enum",
    "description": "Enum utility classes.",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "myclabs/php-enum": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EnhancedReality\\Enum\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EnhancedReality\\Enum\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Sobtchouk"
        }
    ],
    "minimum-stability": "dev"
}
